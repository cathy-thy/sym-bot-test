# For more information, please refer to deployment documentation:
# https://docs.developers.symphony.com/bots/getting-started/wdk#configuration

bdk:
  host: <%= host %>
  bot:
    username: <%= username %>
    privateKey.path: rsa/privatekey.pem
  app:
    appId: <%= appId %>
    privateKey.path: rsa/privatekey.pem
wdk:
  encrypt.passphrase: <%= randomKey %>
  properties:
    monitoring-token: <%= randomKey %>
    management-token: <%= randomKey %>
  studio:
    github-token: github_token
    admins: 1234567,2345678
